{
    "name": "amirpourastarabadi/laravel-mysql-spatial-1",
    "description": "MySQL spatial data types extension for Laravel.",
    "scripts": {
        "test": "phpunit -c phpunit.xml.dist",
        "test:unit": "phpunit -c phpunit.xml.dist --testsuite unit",
        "test:integration": "phpunit -c phpunit.xml.dist --testsuite integration"
    },
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "ext-pdo": "*",
        "ext-json": "*",
        "illuminate/database": "^8.0|^9",
        "geo-io/wkb-parser": "^1.0",
        "jmikola/geojson": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.5",
        "laravel/laravel": "^8.0|^9",
        "doctrine/dbal": "^2.5",
        "laravel/browser-kit-testing": "^2.0",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "MysqlSpatial\\LaravelMysqlSpatial\\": "src/"
        }
    },
    "autoload-dev" : {
        "classmap" : [
            "tests/Unit",
            "tests/Integration"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        },
        "laravel": {
            "providers": [
                "MysqlSpatial\\LaravelMysqlSpatial\\SpatialServiceProvider"
            ]
        }
    }
}
